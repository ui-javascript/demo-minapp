{
	"name": "taro-demo",
	"version": "1.0.0",
	"private": true,
	"description": "Taro商场",
	"main": "index.js",
	"scripts": {
		"commitmsg": "validate-commit-msg",
		"commit": "git-cz",
		"dev:weapp": "taro build --type weapp --watch",
		"dev:h5": "taro build --type h5 --watch",
		"dev:rn": "taro build --type rn --watch",
		"dev:alipay": "taro build --type alipay --watch",
		"dev:swan": "taro build --type swan --watch",
		"build": "cross-env NODE_ENV=production taro build",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"config": {
		"validate-commit-msg": {
			"types": [
				"feat",
				"fix",
				"docs",
				"style",
				"refactor",
				"perf",
				"test",
				"chore",
				"revert",
				"ci"
			],
			"warnOnFail": false,
			"maxSubjectLength": 100,
			"helpMessage": ""
		}
	},
	"author": "O2TEAM",
	"license": "MIT",
	"dependencies": {
		"@tarojs/async-await": "1.1.8",
		"@tarojs/components": "1.1.8",
		"@tarojs/redux": "1.1.8",
		"@tarojs/redux-h5": "1.1.8",
		"@tarojs/rn-runner": "1.1.8",
		"@tarojs/router": "1.1.8",
		"@tarojs/taro": "1.1.8",
		"@tarojs/taro-alipay": "^1.1.8",
		"@tarojs/taro-h5": "1.1.8",
		"@tarojs/taro-weapp": "1.1.8",
		"classnames": "^2.2.5",
		"global": "^4.3.2",
		"leancloud-realtime": "^4.2.0",
		"leancloud-storage": "^3.10.0",
		"nerv-redux": "^1.2.18",
		"nervjs": "^1.2.17",
		"redux": "^4.0.0",
		"redux-actions": "^2.3.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.2.0",
		"tcb-admin-node": "^1.0.21"
	},
	"devDependencies": {
		"@tarojs/plugin-babel": "1.1.8",
		"@tarojs/plugin-csso": "1.1.8",
		"@tarojs/plugin-sass": "1.1.8",
		"@tarojs/plugin-uglifyjs": "1.1.8",
		"@tarojs/webpack-runner": "1.1.8",
		"babel-eslint": "^10.0.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"commitizen": "^2.9.6",
		"cross-env": "^5.1.4",
		"cz-conventional-changelog": "^2.1.0",
		"eslint": "^4.19.1",
		"eslint-config-standard": "^11.0.0",
		"eslint-config-taro": "1.1.8",
		"eslint-plugin-flowtype": "^2.46.1",
		"eslint-plugin-import": "^2.9.0",
		"eslint-plugin-jest": "^21.15.0",
		"eslint-plugin-node": "^6.0.1",
		"eslint-plugin-promise": "^3.7.0",
		"eslint-plugin-react": "^7.7.0",
		"eslint-plugin-standard": "^3.0.1",
		"husky": "^0.14.3",
		"lint-staged": "^7.0.4",
		"validate-commit-msg": "^2.14.0"
	}
}
